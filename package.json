{
  "name": "f2e-front",
  "version": "1.0.0",
  "main": "./main.tsx",
  "author": "F2E Team",
  "license": "MIT",
  "scripts": {
    "rm-public": "rm -rf ./public",
    "rm-source-maps": "find ./public -name *.js.map -type f -delete",
    "dev": "cross-env API_URL=/api/ vite --port 8080 --host",
    "dev:local-api": "cross-env API_PROXY_PORT=3000 yarn dev",
    "build": "yarn rm-public && vite build && yarn run rm-source-maps",
    "deploy": "gh-pages -d public",
    "preview": "vite preview --port 8080",
    "tsc:check": "tsc --project tsconfig.json",
    "lint": "eslint --ignore-path .gitignore . --ext ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --ignore-path .gitignore . --ext ts,.tsx,.js,.jsx --fix ",
    "stylelint": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\" --fix",
    "format": "yarn lint:fix && yarn stylelint:fix",
    "check-circular-deps": "bash scripts/check-circular-deps.sh",
    "check-circular-deps:json": "bash scripts/check-circular-deps-json.sh",
    "precommit": "yarn check-circular-deps && yarn tsc:check && lint-staged",
    "prepare": "husky",
    "minify-images": "node --loader ts-node/esm tools/images/imagemin.mts"
  },
  "homepage": "./",
  "commands": [
    "yarn build"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,scss,css}": [
      "yarn prettier --write"
    ],
    "*.{ts,js}?(x)": [
      "yarn lint"
    ],
    "*.{scss,css}": [
      "yarn stylelint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@sentry/react": "^8.9.2",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "clsx": "^2.0.0",
    "eruda": "^3.0.1",
    "framer-motion": "^10.16.5",
    "gh-pages": "^6.3.0",
    "history": "^5.3.0",
    "mobx": "^6.11.0",
    "mobx-react": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/lint": "^18.5.0",
    "@sentry/vite-plugin": "^2.19.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@swc/plugin-styled-components": "^1.5.119",
    "@types/imagemin-mozjpeg": "^8.0.4",
    "@types/imagemin-svgo": "^10.0.5",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "commitlint-plugin-function-rules": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "glob": "^10.3.10",
    "husky": "^9.0.11",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^15.2.2",
    "madge": "^8.0.0",
    "postcss": "^8.4.38",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "react-is": "^18.2.0",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-declaration-strict-value": "^1.10.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-svgr": "^4.2.0"
  }
}
